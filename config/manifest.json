{
  "manifest_version": 2,
  "name": "扩展练习",
  "description": "这是一个用于练习的扩展程序",
  "version": "1.0",
  "minimum_chrome_version": "80",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "bookmarks",
    "cookies",
    "history",
    "chrome://favicon/*"
  ],
  "chrome_url_overrides": {
    "newtab": "./newtab.html"
  },
  "browser_action": {
    "default_title": "标签页管理",
    "default_popup": "./popup.html"
  },
  "background": {
    "scripts": [
      "./background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}